{
  "name": "ask-the-stars",
  "version": "1.0.0",
  "description": "길거리 예술가 후원 플랫폼",
  "private": true,
  "scripts": {
    "install:all": "npm install && cd front && npm install && cd ../back && npm install",
    "front:dev": "cd front && npm run dev",
    "front:build": "cd front && npm run build",
    "front:preview": "cd front && npm run preview",
    "backend:dev": "cd back && npm run dev",
    "backend:build": "cd back && npm run build",
    "backend:start": "cd back && npm start",
    "dev": "concurrently \"npm run backend:dev\" \"npm run front:dev\"",
    "build": "npm run backend:build && npm run front:build",
    "start": "npm run backend:start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "workspaces": [
    "front",
    "back"
  ],
  "keywords": [
    "street-artist",
    "support",
    "platform",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "",
  "license": "MIT"
}
